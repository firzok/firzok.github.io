  "dependencies": {
    "chart.js": "2.9.4",
    "classnames": "2.2.6",
    "gh-pages": "^2.1.1",
    "moment": "2.24.0",
    "node-sass": "^4.14.1",
    "nouislider": "13.1.1",
    "perfect-scrollbar": "1.4.0",
    "react": "16.8.3",
    "react-bootstrap-switch": "15.5.3",
